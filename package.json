{
  "name": "bnext-challenge",
  "version": "1.0.0",
  "description": "Bnext challenge",
  "author": "Acid Tango",
  "private": true,
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "containers:restart": "docker-compose down && docker volume prune -f && docker-compose up -d db",
    "initialize:db": "yarn containers:restart && sleep 4 && yarn migration:run",
    "format:check": "prettier --check \"src/**/*.ts\" \"test/**/*.ts\" \"**/*.json\"",
    "format:fix": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\" \"**/*.json\"",
    "format:ci": "yarn format:check",
    "start": "rimraf dist && nest start",
    "start:dev": "rimraf dist && nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/src/main",
    "typecheck": "tsc",
    "lint:check": "eslint --max-warnings 0 \"{src,apps,libs,test}/**/*.ts\"",
    "lint:fix": "eslint --fix \"{src,apps,libs,test}/**/*.ts\"",
    "lint:ci": "yarn lint:check",
    "test": "jest -c test/config/unitary.json",
    "test:integration": "jest -c test/config/integration.json",
    "test:unitary:db": "jest -c test/config/unitary-db.json",
    "test:all:db": "jest -c test/config/all-db.json",
    "test:coverage": "yarn test:all:db --coverage",
    "test:ci": "yarn test:coverage",
    "typeorm": "ts-node ./node_modules/typeorm/cli -d src/database/orm.config.ts",
    "migration:generate": "yarn typeorm migration:generate src/database/migrations/migration",
    "migration:run": "yarn typeorm migration:run",
    "migration:revert": "yarn typeorm migration:revert",
    "precommit": "yarn typecheck && yarn format:fix && yarn lint:fix",
    "run-checks": "yarn typecheck && yarn format:check && yarn lint:check"
  },
  "engines": {
    "node": "^16.13.0",
    "yarn": "~1.22.5"
  },
  "dependencies": {
    "@nestjs/axios": "0.0.7",
    "@nestjs/common": "8.4.4",
    "@nestjs/core": "8.4.4",
    "@nestjs/platform-express": "8.4.4",
    "@nestjs/swagger": "5.2.1",
    "@nestjs/terminus": "8.0.6",
    "@nestjs/typeorm": "8.1.4",
    "@swc/core": "1.2.206",
    "@swc/jest": "0.2.21",
    "class-transformer": "0.5.1",
    "class-validator": "0.13.2",
    "express": "4.18.1",
    "leaked-handles": "5.2.0",
    "morgan": "1.10.0",
    "nestjs-pino": "2.5.2",
    "pg": "^8.8.0",
    "pino-http": "6.6.0",
    "rimraf": "3.0.2",
    "rxjs": "7.5.5",
    "swagger-ui-express": "4.3.0",
    "typeorm": "0.3.11",
    "uuid": "8.3.2"
  },
  "devDependencies": {
    "@nestjs/cli": "8.2.5",
    "@nestjs/schematics": "8.0.10",
    "@nestjs/testing": "8.4.4",
    "@shopify/eslint-plugin": "41.2.1",
    "@types/express": "4.17.13",
    "@types/jest": "27.5.0",
    "@types/morgan": "1.9.3",
    "@types/node": "^18.7.18",
    "@types/pino-http": "5.8.1",
    "@types/supertest": "2.0.12",
    "@types/uuid": "8.3.4",
    "@typescript-eslint/eslint-plugin": "5.22.0",
    "@typescript-eslint/parser": "5.22.0",
    "eslint": "8.14.0",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-config-airbnb-typescript": "17.0.0",
    "eslint-config-import": "0.13.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-prettier": "4.0.0",
    "jest": "28.0.3",
    "prettier": "2.6.2",
    "supertest": "6.2.3",
    "tepper": "0.4.2",
    "ts-loader": "9.3.0",
    "ts-node": "10.7.0",
    "tsconfig-paths": "4.0.0",
    "typescript": "4.7.4"
  }
}
